{
  "name": "credit-jambo-api",
  "version": "1.0.0",
  "description": "Credit Jambo Backend API",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/server.js",
    "build": "tsc -p .",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:all": "npm run db:push && npm run db:generate && npm run db:seed",
    "dev": "nodemon ./src/server.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPatterns=\\.test\\.ts$",
    "test:integration": "jest --testPathPatterns=\\.integration\\.test\\.ts$",
    "db:seed": "ts-node ./prisma/seed.ts",
    "watch": "tsc -w"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongodb": "^6.8.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.15",
    "prisma": "^5.19.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.5.4",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.15",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "jiti": "^2.6.1",
    "nodemon": "^3.1.4",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.46.2"
  }
